    ],
    "moduleNameMapper": {
      "\\.svg": "<rootDir>/__mocks__/svgMock.js",
      "\\.png": "<rootDir>/__mocks__/pngMock.js"
    }
  },
  "rnpm": {
    "assets": [
      "./assets/fonts/"
    ]
  }
